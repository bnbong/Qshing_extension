{
  "name": "qshing-defender",
  "version": "1.0.0",
  "description": "Browser extension that protects users from phishing attacks",
  "main": "manifest.json",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:css": "stylelint '**/*.css'",
    "lint:css:fix": "stylelint '**/*.css' --fix",
    "format": "prettier --write '**/*.{js,json,css,html,md}'",
    "format:check": "prettier --check '**/*.{js,json,css,html,md}'",
    "lint:all": "npm run lint && npm run lint:css",
    "fix:all": "npm run lint:fix && npm run lint:css:fix && npm run format",
    "prepare": "command -v pre-commit >/dev/null 2>&1 && pre-commit install || echo 'pre-commit not found, skipping hook installation'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bnbong/Qshing_extension.git"
  },
  "keywords": [
    "browser-extension",
    "phishing-protection",
    "security",
    "chrome-extension",
    "web-security"
  ],
  "author": "bnbong",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "9.19.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.4.1",
    "globals": "15.14.0",
    "prettier": "3.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ]
}
